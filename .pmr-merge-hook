#!/bin/sh

set -e

git clone https://git.launchpad.net/~checkbox-dev/checkbox/+git/support
time -o timing.dat ./support/test-in-lxc.sh
echo "Complete test time:"
cat timing.dat
./support/lp-snap-build checkbox-snappy -o ce-certification-qa -a amd64
./support/lp-snap-build checkbox-snappy -o ce-certification-qa -a armhf
